<tr>
    <td><%= user.id %></td>
    <td><%= link_to user.name, admin_user_path(user) %></td>
    <td><%= user.email %></td>
    <td><%= user.signed_up_at %></td>
    <td><%= user.last_updated_at %></td>
    <td><%=  gravatar_for(user,size: 20) %></td>
    <td>
        <%= link_to "Edit", edit_admin_user_path(user) %>
        <%= link_to "Delete", admin_user_path(user), data: {method: :delete}, confirm: "Are you sure?" %>
    </td>
</tr>
